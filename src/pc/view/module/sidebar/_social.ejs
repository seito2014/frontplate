<div class="social l-grid l-grid-col2">
    <ul class="l-grid-inner">
        <% for(var iSns=0; iSns< snsArray.length; iSns++) { %>
        <li class="l-grid-item">
            <a href="<%= snsArray[iSns].url %>" target="_blank">
                <i class="sprite-sns-2x sprite-sns-<%= snsArray[iSns].name %>"></i>
            </a>
        </li>
        <% } %>
    </ul>
</div>